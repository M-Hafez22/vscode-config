	"Print to console": {
		"prefix": "clog",
		"body": [
			"console.log('$1');",
			"$2"
		],
		"description": "Log output to console"
	},
	"const": {
		"prefix": "c",
		"body": [
			"const"
		],
		"description": "Log output to console"
	},
	"useState Hook": {
		"prefix": "ustat",
		"body": [
			"const [$1, set$1] = useState('$2');$0"
		],
		"description": "useState Hook"
	},
	"Fetch try catch": {
		"prefix": "fetch",
		"body": [
			"try{",
			"\tconst res = await fetch('$URL');",
			"\tconst data = await res.json();",
			"\t$0",
			"} catch (err) {",
			"\tconsole.log(err)",
			"}"
		],
		"description": "Fetch with try catch"
	},
}